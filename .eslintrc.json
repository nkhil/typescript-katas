{
  "env": {
      "es2021": true,
      "jest": true
  },
  "extends": [
    "airbnb-typescript",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "project": ["./tsconfig.json"]
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
    "react/jsx-filename-extension":"off",
    "eol-last": ["error", "always"]
  }
}
